<div class="topology dashboard-item">

	<div class="timeago">updated <%= timeago( new Date(Date.parse(topology.updated_at)) ) %></div>

	<a href="/topologies/<%= topology._id.toHexString() %>"><%= topology.name %></a>
	<p>
	<% if(!!topology.config && !!topology.config.properties && topology.config.properties.description) { %>
			<%= topology.config.properties.description %>
	<% } else { %>
			No description yet
	<% } %>
	</p>

</div>